{
  "name": "simp-simp_bolt",
  "version": "0.1.1",
  "author": "SIMP Team",
  "summary": "A SIMP module to manage Puppetlabs Bolt",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-simp_bolt",
  "project_page": "https://github.com/simp/pupmod-simp-simp_bolt",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "bolt"
  ],
  "dependencies": [
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.14.1 < 4.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.25.1 < 7.0.0"
    }
  ],
  "simp": {
    "optional_dependencies": [
      {
        "name": "simp/pam",
        "version_requirement": ">= 6.3.0 < 7.0.0"
      },
      {
        "name": "simp/sudo",
        "version_requirement": ">= 5.1.1 < 6.0.0"
      }
    ]
  },
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.5.10 < 7.0.0"
    }
  ]
}
